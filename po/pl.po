#: config.cc:32
msgid "Unable to parse command-line options"
msgstr "Błąd przetwarzania opcji wiersza poleceń"

#: config.cc:40
msgid "Too many arguments were specified"
msgstr "Podano za dużo argumentów"

#: config.cc:48
msgid "No input file name was specified"
msgstr "Nie podano nazwy pliku wejściowego"

#: config.cc:56
msgid "Invalid output file name"
msgstr "Błędna nazwa pliku wyjściowego"

#: config.cc:64
msgid "Unable to parse page numbers"
msgstr "Błąd przetwarzania numerów stron"

#: config.cc:72
msgid "Unable to parse page size"
msgstr "Błąd przetwarzania rozmiaru strony"

#: config.cc:80
msgid "Unable to parse hyperlinks options"
msgstr "Błąd przetwarzania opcji hiperłączy"

#: config.cc:88
#, c-format
msgid "The specified resolution is outside the allowed range: %d .. %d"
msgstr "Podana rozdzielczość jest spoza dozwolonego zakresu: %d .. %d"

#: config.cc:96
#, c-format
msgid ""
"The specified number of foreground colors is outside the allowed range: %"
"u .. %u"
msgstr ""
"Podana liczba kolorów pierwszego planu jest spoza dozwolonego zakresu: %u .. "
"%u"

#: config.cc:104
#, c-format
msgid "The specified subsampling ratio is outside the allowed range: %u .. %u"
msgstr ""
"Podany współczynnik próbkowania jest spoza dozwolonego zakresu: %u .. %u"

#: config.cc:112
msgid "Unable to parse page title template specification"
msgstr "Błąd przetwarzania specyfikacji szablonu tytułu strony"

#: config.cc:120
msgid "Unable to parse pageid template specification"
msgstr "Błąd przetwarzania specyfikacji szablonu pageid"

#: config.cc:128
msgid ""
"Pageid must consist only of letters, digits, '_', '+', '-' and '.' characters"
msgstr ""
"Pageid może zawierać jedynie litery, cyfry oraz znaki '_', '+', '-' i '.'"

#: config.cc:136
msgid ""
"Pageid cannot start with a '.' character or contain two consecutive '.' "
"characters"
msgstr ""
"Pageid nie może rozpoczynać się od znaku '.' ani zawierać dwóch "
"występujących po sobie znaków '.'"

#: config.cc:144
msgid "Pageid must end with the '.djvu' or the '.djv' extension"
msgstr "Pageid musi kończyć się rozszerzeniem '.djvu' lub '.djv'"

#: config.cc:304
#, c-format
msgid "%s is not a valid number"
msgstr "%s nie jest poprawną liczbą"

#: config.cc:619
msgid "Unknown option"
msgstr "Nieznana opcja"

#: config.cc:638
msgid "Usage: "
msgstr "Użycie: "

#: config.cc:639
msgid "   pdf2djvu [-o <output-djvu-file>] [options] <pdf-file>"
msgstr "   pdf2djvu [-o <plik-wyjściowy-djvu>] [opcje] <plik-pdf>"

#: config.cc:640
msgid "   pdf2djvu  -i <index-djvu-file>   [options] <pdf-file>"
msgstr "   pdf2djvu -i <plik-indeksowy-djvu> [opcje] <plik-pdf>"

#: config.cc:641
msgid "Options: "
msgstr "Opcje: "

#: config.cc:642
msgid " -i, --indirect=FILE"
msgstr " -i, --indirect=PLIK"

#: config.cc:643
msgid " -o, --output=FILE"
msgstr " -o, --output=PLIK"

#: config.cc:644
msgid "     --pageid-prefix=NAME"
msgstr "     --pageid-prefix=NAZWA"

#: config.cc:645
msgid "     --pageid-template=TEMPLATE"
msgstr "     --pageid-template=SZABLON"

#: config.cc:646
msgid "     --page-title-template=TEMPLATE"
msgstr "     --page-title-template=SZABLON"

#: config.cc:647
msgid " -d, --dpi=RESOLUTION"
msgstr " -d, --dpi=ROZDZIELCZOŚĆ"

#: config.cc:650
msgid "     --page-size=WxH"
msgstr "     --page-size=SZERxWYS"

#: config.cc:674
msgid "     --filter-text=COMMAND-LINE"
msgstr "     --filter-text=LINIA-POLECEŃ"

